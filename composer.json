{
    "name": "everyworkflow/media-manager-bundle",
    "description": "Media manager bundle for every workflow project.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "EveryWorkflow",
            "email": "everyworkflow@gmail.com"
        }
    ],
    "require": {
        "symfony/finder": "^4.4|^5.0|^6.0",
        "nesbot/carbon": "^2.55",
        "imagine/imagine": "^1.2",
        "liip/imagine-bundle": "^2.7",
        "everyworkflow/core-bundle": "dev-main"
    },
    "autoload": {
